{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/notes.html",
      "dest": "/public/notes.html"
    },
    {
      "src": "/upload.html",
      "dest": "/public/upload.html"
    },
    {
      "src": "/index.css",
      "dest": "/public/index.css"
    },
    {
      "src": "/notes.js",
      "dest": "/public/notes.js"
    },
    {
      "src": "/upload.js",
      "dest": "/public/upload.js"
    },
    {
      "src": "/motivation.js",
      "dest": "/public/motivation.js"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}
